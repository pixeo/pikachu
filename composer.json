{
    "name": "laravel-zero/laravel-zero",
    "description": "Laravel Zero provides the perfect starting point for your next Laravel Console Application.",
    "keywords": ["framework", "laravel", "console", "zero"],
    "homepage": "http://laravel-zero.com",
    "license": "MIT",
    "support": {
        "issues": "https://github.com/laravel-zero/laravel-zero/issues",
        "source": "https://github.com/laravel-zero/laravel-zero"
    },
    "authors": [
        {
            "name": "Nuno Maduro",
            "email": "enunomaduro@gmail.com"
        }
    ],
    "require": {
        "php": ">=7.1",
        "guzzlehttp/guzzle": "^6.3",
        "laravel-zero/framework": "4.0.*",
        "symfony/css-selector": "^4.0",
        "symfony/dom-crawler": "^4.0",
        "t1gor/robots-txt-parser": "^0.2.3",
        "erusev/parsedown": "^1.6"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require-dev": {
        "phpunit/phpunit": "~6.4",
        "symfony/thanks": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        },
        "files": [
            "app/Support/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true
    },
    "scripts": {
        "post-create-project-cmd": [
            "php application app:rename"
        ]
    },
    "bin": ["pikachu"]
}
